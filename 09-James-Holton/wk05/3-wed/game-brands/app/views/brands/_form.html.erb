<%= form_for @brand do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :founded %>
    <%= f.text_field :founded %>

    <%= f.label :based %>
    <%= f.text_field :based %>

    <%= f.label :net %>
    <%= f.text_field :net %>

    <%= f.label :image %>
    <%= f.url_field :image %>

    <%= f.submit %>
<% end %>